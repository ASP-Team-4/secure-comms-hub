<form action="/customer/<%=customerID%>/bookedcallback" method="POST">
  <% for(let i = 0; i < slots.length; i++) { %>
  <h4>
    <%= slots[i]["day"]%>
    <%=slots[i]["date"]%>/<%=slots[i]["month"]%>/<%=slots[i]["year"]%>
  </h4>
  <% for(let j = 0; j < times.length; j++) { %> 
    <input type="radio" name="time" value="<%=slots[i]["year"]%>-<%=slots[i]["month"]%>-<%=slots[i]["date"]%> <%=times[j]%>"
  /><%=times[j]%></input >
  <br />
  <% }; %> 
  <% }; %>
  <input type="submit" />
</form>

